  "incognito": "split",
  "manifest_version": 2,
  "minimum_chrome_version": "38.0",
  "name": "uBlock Origin dev build",
  "optional_permissions": [
    "file:///*"
  ],
  "options_page": "dashboard.html",
  "options_ui": {
    "page": "options_ui.html"
  },
  "permissions": [
    "contextMenus",
    "privacy",
    "storage",
    "tabs",
    "unlimitedStorage",
    "webNavigation",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "short_name": "uBlock\u2080 dev build",
  "storage": {
    "managed_schema": "managed_storage.json"
  },
  "version": "1.14.17.10"
}
